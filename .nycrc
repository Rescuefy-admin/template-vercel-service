{
  "exclude": [
    "tests/",
    "coverage/",
    ".eslintrc.js",
    "actions/",
    "api/not-found/message.js",
    "api/index.js",
    "config/"
  ],
  "extension": [
    ".js"
  ],
  "cache": true,
  "all": true,
  "default-excludes": true,
  "check-coverage": true,
  "statements": 95,
  "branches": 95,
  "functions": 95,
  "lines": 95,
  "skip-full": true
}
